#include <bits/stdc++.h>
using namespace std;
 
int main() {
  int64_t N;
  cin >> N;
  int i = 0;
  while(N > pow(2,i)){
    i++;
  }
  i = i - 1;
  cout << i;
  
  if(N == pow(2,i+1) - 1){
    if(i%2 == 0){
      cout << "Aoki" << endl;
      return 0;
    }
    if(i%2 != 0){
      cout << "Takahasi" << endl;
      return 0;
    }
  }
  if(N != pow(2,i+1) - 1){
    if(i%2 == 0){
      cout <<  "Takahashi" << endl;
    }
    if(i%2 != 0){
      cout << "Aoki" << endl;
    }
  }
}
    無理
