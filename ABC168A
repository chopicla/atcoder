#include <bits/stdc++.h>
using namespace std;

#A
int main() {
  string N;
  cin >> N;


  if(N.at(N.size()-1) == '3'){
    cout << "bon" << endl;
  }
  if(N.at(N.size()-1) == '0' || N.at(N.size()-1) == '1' || N.at(N.size()-1) == '6' || N.at(N.size()-1) == '8'){
    cout << "pon" << endl;
  }
  if(N.at(N.size()-1) == '2' || N.at(N.size()-1) == '4' || N.at(N.size()-1) == '5' || N.at(N.size()-1) == '7' || N.at(N.size()-1) == '9'){
    cout << "hon" << endl;
  }
}
