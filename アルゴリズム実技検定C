#include <bits/stdc++.h>
using namespace std;
int main() {
  long long A, R, N;
  cin >> A >> R >> N;
  
  long long S = 1;
 
  if(R == 1){
    cout << A << endl;
    return 0;
  }
  else{
    for(long long i = 1; i <= N; i++){
      if(S*R <= 10000000000){
        S  = A * pow(R,i-1);
      }
      else{
        cout << "large" <<endl;
        return 0;
      }
    }
    cout << S << endl;
  }
}
