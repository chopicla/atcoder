#include <bits/stdc++.h>
using namespace std;
int main() {
  int64_t A, R, N;
  scanf("%ld %ld %ld", &A, &R, &N);
  
  int64_t S = 1;

  for(int i = 0; i < N; i++){
    if(S <= 10000000000/R){
      S  = A * pow(R,i);
    }
    else{
      cout << "large" <<endl;
      return 0;
    }
  }
  cout << S << endl;
}
